public class StarTriangle {
    public static void main(String[] args) {
        int lines = 5; // Number of lines in the triangle

        for (int i = 1; i <= lines; i++) { // Outer loop for each line
            for (int j = 1; j <= i; j++) { // Inner loop for stars
                System.out.print("*");
            }
            System.out.println(); // Move to the next line after printing stars
        }
    }
}
